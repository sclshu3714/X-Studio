{
  "App": {
    "Environment": "Development"
  },
  "AppSettings": {
    "Name": "XStudio",
    "BaseUrl": "http://localhost:44345"
  },
  //日志配置-不需要修改
  "Serilog": {
    "WriteTo": [
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "reqlogs//{Date}.txt",
          "RestrictedToMinimumLevel": "Warning",
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz}] [{Level}] \n{Message}{NewLine}",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Console"
      }
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    }
  }
}

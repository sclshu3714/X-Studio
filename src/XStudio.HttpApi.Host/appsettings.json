    "ClusterName": "DEFAULT",

    //
    // 摘要:
    //     the name of the group.
    "GroupName": "wxz",

    //
    // 摘要:
    //     the weight of this instance.
    "Weight": "100.0",

    //
    // 摘要:
    //     the name of the service.
    "ServiceName": "XStudio",

    //
    // 摘要 string
    //
    "UserName": "",

    //
    // 摘要 string
    //
    "Password": "",

    //
    // 摘要: string
    //     accessKey
    "AccessKey": "",

    //
    // 摘要:string
    //     secretKey
    "SecretKey": "",

    //
    // 摘要 string
    //
    "RamRoleName": "",

    //
    // 摘要:
    //  ContextPath
    "ContextPath": "nacos",

    //
    // 摘要:
    //     EndPoint
    "EndPoint": "",

    //
    // 摘要:
    //     default timeout, unit is Milliseconds.
    "DefaultTimeOut": 15000,

    //
    // 摘要:
    //     监听配置文件.
    "Listeners": [
      {
        "Optional": false,
        "DataId": "akstream.yaml",
        "Group": "wxz"
      }
    ],

    //
    // 摘要: int
    //     listen interval, unit is millisecond.
    "ListenInterval": 1000,

    //
    // 摘要:
    //     nacos server addresses.
    "ServerAddresses": [
      //"http://127.0.0.1:8848/"
      "http://10.20.4.186:8848/"
    ],

    //
    // 摘要:
    //     Select an IP that matches the prefix as the service registration IP like the
    //     config of spring.cloud.inetutils.preferred-networks
    "PreferredNetworks": "",

    //
    // 摘要 bool
    //
    "ConfigUseRpc": true,


    //
    // 摘要 bool
    //
    "NamingUseRpc": true,


    //
    // 摘要 string
    //
    "NamingLoadCacheAtStart": "",

    //
    // 摘要 string
    //

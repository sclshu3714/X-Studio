    //
    // 摘要:
    //     If instance is enabled to accept request. The default value is true.
    "InstanceEnabled": true
  },

  //限制接口访问速率 限制配置
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "RealIpHeader": "X-Real-IP",
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429,
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "1s",
        "Limit": 3
      }
    ]
